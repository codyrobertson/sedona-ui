{
  "name": "sedona-ui-kit",
  "version": "1.0.0",
  "description": "Production-ready UI component library built with Next.js 14, Tailwind CSS, shadcn/ui, and Zeus design system. Optimized for AI agent trading platforms.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src/components",
    "src/lib",
    "tailwind.config.ts",
    "figma-plugin",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codyrobertson/sedona-ui.git"
  },
  "homepage": "https://github.com/codyrobertson/sedona-ui#readme",
  "bugs": {
    "url": "https://github.com/codyrobertson/sedona-ui/issues"
  },
  "keywords": [
    "react",
    "nextjs",
    "tailwind",
    "ui-components",
    "design-system",
    "typescript",
    "shadcn-ui",
    "zeus-design",
    "trading-ui",
    "ai-agents",
    "figma-plugin",
    "react-figma"
  ],
  "author": "Sedona Team",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "next dev -p 3002",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "build-lib": "tsc && cp -r src/components dist/ && cp -r src/lib dist/",
    "prepublishOnly": "npm run build-lib",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "analyze-components": "npx ts-node scripts/analyze-components.ts",
    "figma:install": "cd figma-plugin/sedona-sync && npm install",
    "figma:build": "cd figma-plugin/sedona-sync && npm run build",
    "figma:dev": "cd figma-plugin/sedona-sync && npm run dev"
  },
  "dependencies": {
    "@paper-design/shaders-react": "^0.0.68",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.7",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "geist": "^1.5.1",
    "lightweight-charts": "^5.1.0",
    "lucide-react": "^0.525.0",
    "motion": "^12.24.0",
    "next": "14.2.5",
    "ogl": "^1.0.11",
    "playwright": "^1.54.1",
    "postprocessing": "^6.38.2",
    "react": "^18",
    "react-dom": "^18",
    "react-figma": "^0.28.0",
    "reactbits-installer": "^2.0.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.182.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/three": "^0.182.0",
    "@vitejs/plugin-react": "^5.1.2",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "jsdom": "^27.4.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "vitest": "^4.0.16"
  }
}
